{
  "name": "@mhriyad/toon-package",
  "version": "1.0.0",
  "description": "Token-Oriented Object Notation - A compact, human-readable encoding of JSON for LLM prompts with 30-60% token savings",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "watch": "tsc --watch",
    "test": "echo \"Error: No tests yet exists\"&& exit 1",
    "test:local": "npm run build && node test-local.mjs && node test-local.cjs",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "toon",
    "json",
    "llm",
    "token",
    "encoding",
    "serialization",
    "compact",
    "typescript",
    "ai",
    "prompt",
    "efficient"
  ],
  "author": "mhriyad",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "esbuild": "^0.27.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/includexxx/toon.git"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
